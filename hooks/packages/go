#!/bin/bash
set -e

export GOPATH=${GOPATH:-"${HOME}/.go"}

PACKAGES=""
function __add_package() {
  PACKAGES="$1 $PACKAGES"
}

# ----------------------
# | Add packages below |
# ----------------------

__add_package "github.com/aykamko/tag"
__add_package "github.com/simeji/jid/cmd/jid"

# ------------------------
# | Done adding packages |
# ------------------------

cat <<MSG

PACKAGES: $PACKAGES

MSG

go get -u $PACKAGES
